<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>FiatCoinDB</TargetDatabaseName>
    <DeployScriptFileName>DBDeployment.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=fiatcoindb.database.windows.net;Persist Security Info=True;User ID=sqladmin;Pooling=False</TargetConnectionString>
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <ScriptDatabaseOptions>True</ScriptDatabaseOptions>
    <BlockOnPossibleDataLoss>False</BlockOnPossibleDataLoss>
    <CreateNewDatabase>True</CreateNewDatabase>
  </PropertyGroup>
</Project>