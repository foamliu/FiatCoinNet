<Window x:Class="FiatCoinNet.WalletGui.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FiatCoinNet.WalletGui"
        mc:Ignorable="d"
        Title="{DynamicResource WINDOW_TITLE_ABOUT}" Height="250" Width="350">
    <Grid>
        <Image x:Name="image" HorizontalAlignment="Left" Height="63" Margin="22,44,0,0" VerticalAlignment="Top" Width="63" Source="Logo.ico" />
        <Label x:Name="label0" Content="{DynamicResource ABOUT_CONTENT0}" HorizontalAlignment="Left" Margin="99,37,0,0" VerticalAlignment="Top" Width="165" Height="29"/>
        <Label x:Name="label1" Content="{DynamicResource ABOUT_CONTENT1}" HorizontalAlignment="Left" Margin="99,66,0,0" VerticalAlignment="Top" Width="226" Height="29"/>
        <Label x:Name="label2" Content="{DynamicResource ABOUT_CONTENT2}" HorizontalAlignment="Left" Margin="99,95,0,0" VerticalAlignment="Top" Width="165" Height="29"/>
        <Button x:Name="btnOK" Content="OK" HorizontalAlignment="Left" Margin="225,168,0,0" VerticalAlignment="Top" Width="75" Click="btnOK_Click" />

    </Grid>
</Window>
